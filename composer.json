{
  "name": "gohany/circuitbreaker-symfony-bundle",
  "description": "Symfony bundle wiring for Gohany/Circuitbreaker (PHP 7.4, Symfony 5.4+).",
  "type": "symfony-bundle",
  "license": "proprietary",
  "require": {
    "php": ">=7.4",
    "gohany/circuitbreaker": "dev-main",
    "symfony/framework-bundle": "^5.4 || ^6.0 || ^7.0",
    "symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0",
    "symfony/config": "^5.4 || ^6.0 || ^7.0",
    "symfony/console": "^5.4 || ^6.0 || ^7.0",
    "symfony/http-client-contracts": "^2.5 || ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "Gohany\\Circuitbreaker\\bundle\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-main": "0.1-dev"
    }
  }
}
