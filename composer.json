{
  "name": "gohany/circuitbreaker-symfony-bundle",
  "description": "Symfony bundle wiring for Gohany/Circuitbreaker (PHP 7.4, Symfony 5.4+).",
  "type": "symfony-bundle",
  "license": "proprietary",
  "require": {
    "php": ">=7.4",
    "gohany/circuitbreaker": "v1.1.0",
    "symfony/framework-bundle": "^5.4 || ^6.0 || ^7.0",
    "symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0",
    "symfony/config": "^5.4 || ^6.0 || ^7.0",
    "symfony/console": "^5.4 || ^6.0 || ^7.0",
    "symfony/http-client-contracts": "^2.5 || ^3.0"
  },
  "require-dev": {
    "doctrine/dbal": "^3.0",
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "Gohany\\CircuitBreakerSymfonyBundle\\": "src/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit -c phpunit.xml.dist",
    "test:unit": "vendor/bin/phpunit -c phpunit.xml.dist",
    "test:integration": "vendor/bin/phpunit -c phpunit.integration.xml.dist",
    "test:coverage": "mkdir -p build/logs && vendor/bin/phpunit -c phpunit.xml.dist --coverage-clover build/logs/clover.xml"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-main": "0.1-dev"
    }
  }
}
